{
  "name": "must-be-check",
  "version": "1.0.5",
  "description": "Must-Be-Check is a TypeScript generic validation tool.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "npx jest",
    "tsc": "node -e \"require('del')('./build')\" & npx tsc",
    "start": "node -r ./ts-node.config.js src/index.ts",
